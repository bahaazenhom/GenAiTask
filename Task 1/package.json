{
  "name": "quicksort-lab-assignment",
  "version": "1.0.0",
  "description": "Comprehensive QuickSort implementation with GitHub Copilot assistance",
  "main": "quicksort.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "benchmark": "node benchmark.js",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": [
    "quicksort",
    "sorting",
    "algorithms",
    "github-copilot",
    "javascript"
  ],
  "author": "GitHub Copilot Lab Assignment",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "*.js",
      "!*.test.js",
      "!server.js",
      "!app.js",
      "!benchmark.js",
      "!coverage/**"
    ],
    "testMatch": [
      "**/*.test.js"
    ]
  }
}
